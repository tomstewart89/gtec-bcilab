		<Event id="noflash">
			<TextSymbol>
				<ScaleSize>0.6</ScaleSize>
				<ForegroundColor>20,20,20</ForegroundColor>
				<BackgroundColor>10,10,10</BackgroundColor>
				<Source>openvibe/applications/CoAdaptP300Stimulator/VeraMono-Bold.ttf</Source>
				<Label>A</Label>
			</TextSymbol>
		</Event>
		<Event id="flash">
			<PictureSymbol>
				<ScaleSize>0.85</ScaleSize>
				<ForegroundColor>70,70,70</ForegroundColor>
				<BackgroundColor>10,10,10</BackgroundColor>
